{
    "name": "gurgentil/eloquent-filters",
    "description": "A Laravel package to add query filters to your Eloquent models with ease",
    "keywords": [
        "laravel",
        "eloquent",
        "filters",
        "query"
    ],
    "homepage": "https://github.com/gurgentil/eloquent-sequencer",
    "authors": [
        {
            "name": "Gustavo Rorato Gentil"
        }
    ],
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^7.2",
        "illuminate/support": "^7.0|^8.0"
    },
    "require-dev": {
        "orchestra/testbench": "^5.0|^6.0"
    },
    "autoload": {
        "psr-4": {
            "Gurgentil\\EloquentFilters\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Gurgentil\\EloquentFilters\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Gurgentil\\EloquentFilters\\ServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
