{
    "name": "gurgentil/eloquent-filters",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^7.2|^8.0",
        "illuminate/support": "^7.0|^8.0"
    },
    "require-dev": {
        "orchestra/testbench": "^5.0|^6.0"
    },
    "autoload": {
        "psr-4": {
            "Gurgentil\\EloquentFilters\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Gurgentil\\EloquentFilters\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Gurgentil\\EloquentFilters\\ServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
